<p id="notice"><%= notice %></p>

<h1>Tipo Repartos</h1>

<table>
  <thead>
    <tr>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_repartos.each do |tipo_reparto| %>
      <tr>
        <td><%= tipo_reparto.descripcion %></td>
        <td><%= link_to 'Show', tipo_reparto %></td>
        <td><%= link_to 'Edit', edit_tipo_reparto_path(tipo_reparto) %></td>
        <td><%= link_to 'Destroy', tipo_reparto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Reparto', new_tipo_reparto_path %>
